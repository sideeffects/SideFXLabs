--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/polynomial_fitting1/attribwrangle2.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int neighbours[] = i[]@neighbours;    

// create orthonormal basis
matrix3 mat_p = matrix3(ident()) - outerproduct(@N, @N);
vector vec_u = normalize((point(@OpInput1, \"P\", neighbours[0]) - @P) * mat_p);
vector vec_v = normalize(cross(@N, vec_u));

// initialize arrays
vector a_array[];
float b_array[];

// fit quadratic polynomial to neighbouring points
foreach(int i; int neighbour; neighbours)
{
    vector pos = point(@OpInput1, \"P\", neighbour);
    vector vec = (pos - @P) * mat_p;
    float u = dot(vec, vec_u);
    float v = dot(vec, vec_v);
    a_array[i] = set(pow(u, 2), u * v, pow(v, 2));
    b_array[i] = dot(pos - @P, @N);    
}

// initialize values
vector temp_array[];
resize(temp_array, 3);
vector vec_temp = 0;

// build matrices
int i = 3;
while(i--)
{
    for(int j = 0; j < 3 ; j++)
    {
        for(int k = 0; k < len(a_array); k++)
            temp_array[i][j] += a_array[k][i % 3] * a_array[k][j];
    }
}
matrix3 mat_a = set(temp_array);

i = 3;
while(i--)
{
    for(int j = 0; j < len(a_array); j++)
        vec_temp[i] += a_array[j][i] * b_array[j];
}
matrix3 mat_b = 0;
for(i = 0; i < 3; i++)
        setcomp(mat_b, vec_temp[i], i, 0);

        
// get coefficients        
matrix3 coeff_mat = invert(mat_a) * mat_b;
vector coeff = set(coeff_mat.xx, coeff_mat.yx, coeff_mat.zx);
float a = coeff.x, b = coeff.y, c = coeff.z;


// compute curvatures
@max_curv = coeff.x + coeff.z + sqrt((pow(coeff.x - coeff.z, 2) + pow(coeff.y, 2)));
