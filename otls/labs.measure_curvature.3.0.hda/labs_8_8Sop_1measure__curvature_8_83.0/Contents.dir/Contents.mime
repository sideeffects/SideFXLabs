snippet	[ 0	locks=0 ]	(	"// approximate attributes on boundary points

int neighbours[] = neighbours(@OpInput1, @ptnum);
int num = len(neighbours);

float mean_curv = 0;
float gauss_curv = 0;
float max_curv = 0;
float min_curv = 0;
vector max_curv_dir = 0;
vector min_curv_dir = 0;
int count = 0;

foreach(int neighbour; neighbours)
{
    if(!inpointgroup(@OpInput1, \"boundary\", neighbour))
    {
        mean_curv += point(@OpInput1, \"mean_curv\", neighbour);
        gauss_curv += point(@OpInput1, \"gauss_curv\", neighbour);
        max_curv += point(@OpInput1, \"max_curv\", neighbour);
        min_curv += point(@OpInput1, \"min_curv\", neighbour);
        vector vec = point(@OpInput1, \"min_curv_dir\", neighbour);
        if(dot(vec, min_curv_dir) < 0)
            vec *= -1;
        min_curv_dir += vec;
        vec = point(@OpInput1, \"max_curv_dir\", neighbour);
        if(dot(vec, max_curv_dir) < 0)
            vec *= -1;
        max_curv_dir += vec;
        count += 1;
    }
}

@mean_curv = mean_curv / count;
@gauss_curv = gauss_curv / count;
@max_curv = max_curv / count;
@min_curv = min_curv / count;
v@min_curv_dir = normalize(min_curv_dir);
v@max_curv_dir = normalize(max_curv_dir);
f@curvature = @mean_curv;
"	)
