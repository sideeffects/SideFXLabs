snippet	[ 0	locks=0 ]	(	"// compute largest eigenvector
vector2 eigenvector2(const matrix2 mat_input; const int max_iter; const float tol)
{
        matrix2 mat = mat_input;
        vector2 vec = 1;
        vector2 vec_max = normalize(vec * mat);
        vector2 vec_max_temp;
        
        int i = max_iter;
        while(i--)
        {
            vec_max_temp = vec_max;
            vec_max *= mat;
            vec_max = normalize(vec_max);
            int cond = distance(pow(vec_max, 2), pow(vec_max_temp, 2)) < tol;
            if(cond)
                    break;
        }
        return vec_max;
}


// project matrix
float projection(const matrix3 mat_input; const vector vec_u_input; const vector vec_v_input)
{
    vector4 vec_temp = 0;
    for(int c = 0; c < 3 ; c++)
    {
        for(int r = 0; r < 3; r++)
            vec_temp[c] += vec_u_input[r] * getcomp(mat_input, r, c);
    }
    for(int r = 0; r < 3; r++)
        vec_temp[3] += vec_temp[r] * vec_v_input[r];

    return vec_temp[3];
}



int neighbours[] = neighbours(@OpInput1, @ptnum);

// create local orthonormal basis
vector vec_u = normalize({1, 0, 0} * (matrix3(ident()) - matrix3(outerproduct(@N, @N))));
vector vec_v = normalize(cross(@N, vec_u));

// compute shape operator
matrix3 mat_temp = set(v@dx, v@dy, v@dz);
matrix2 mat = set(  projection(mat_temp, vec_u, vec_u), projection(mat_temp, vec_u, vec_v),
                    projection(mat_temp, vec_v, vec_u), projection(mat_temp, vec_v, vec_v));
                    
// compute principal curvature and curvature directions
vector2 eigvec1 = normalize(eigenvector2(mat, 100, 1e-7));
vector2 eigvec2 = normalize(set(eigvec1.y, -eigvec1.x));
float k1 = (matrix2(set(eigvec1 * mat, 0)) * transpose(matrix2(set(eigvec1, 0)))).xx;
float k2 = (matrix2(set(eigvec2 * mat, 0)) * transpose(matrix2(set(eigvec2, 0)))).xx;

// compute curvatures
@mean_curv = (mat.xx + mat.yy) * -0.5;
@gauss_curv = mat.xx * mat.yy - mat.xy * mat.yx;
@max_curv = k1 * -1;
@min_curv = k2 * -1;
v@max_curv_dir = vec_u * eigvec1.x + vec_v * eigvec1.y;
v@min_curv_dir = vec_u * eigvec2.x + vec_v * eigvec2.y;

f@curvature = @mean_curv;
/*  NOTE:
calculating mean and gauss curvature based on k1 and k2 should lead
to the exact same result
*/

//@mean_curv = (@min_curv + @max_curv) * 0.5;
//@gauss_curv = @min_curv * @max_curv;






