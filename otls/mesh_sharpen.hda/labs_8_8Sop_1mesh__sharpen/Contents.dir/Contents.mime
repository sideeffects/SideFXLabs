Content-Disposition: attachment; filename="hdaroot/cl_solve.parm"
Content-Type: text/plain

{
version 0.8
stdswitcher	[ 0	locks=0 ]	(	0	0	0	)
kernelname	[ 0	locks=0 ]	(	meshSharpen	)
usecode	[ 0	locks=0 ]	(	"on"	)
kernelfile	[ 0	locks=0 ]	(	""	)
kernelcode	[ 0	locks=0 ]	(	"kernel void meshSharpen( 
                 int P_length, 
                 global float * P ,
                 int neighbours_length, 
                 global int * neighbours_index, 
                  global exint * neighbours ,
                 int dP_length, 
                 global float * dP ,
                 int curvature_length, 
                 global float * curvature ,
                 float  step_size ,
                 int bound_weight_length , 
                 global float * bound_weight
){
    int idx = get_global_id(0);
    if (idx >= P_length)
        return;
    
    int start = neighbours_index[idx];
    int end = neighbours_index[idx + 1];
    float3 avg = {0.0,0.0,0.0};
    float weight = 0.0;
    
    for(int i = start; i < end; i++){
        int npt = neighbours[i];
        float npt_curvature = curvature[npt];
        float3 pos = vload3(npt, P);
        avg += pos * npt_curvature;
        weight += npt_curvature;
    }
    
    avg /= weight;
    float3 out_p = vload3(idx, P);
    float3 dir = avg - out_p;
    float bweight = bound_weight[idx];
    dir *=  step_size * bweight;
    vstore3(dir, idx, dP);
}

kernel void writeBack( 
                 int P_length, 
                 global float * P ,
                 int neighbours_length, 
                 global int * neighbours_index, 
                  global exint * neighbours ,
                 int dP_length, 
                 global float * dP ,
                 int curvature_length, 
                 global float * curvature ,
                 float  step_size ,
                 int bound_weight_length , 
                 global float * bound_weight
){
    int idx = get_global_id(0);
    if (idx >= P_length)
        return;
        
    float3 delta = vload3(idx, dP);
    float3 out_p = vload3(idx, P);
    out_p = out_p + delta;
    
    vstore3(out_p, idx, P);

}"	)
