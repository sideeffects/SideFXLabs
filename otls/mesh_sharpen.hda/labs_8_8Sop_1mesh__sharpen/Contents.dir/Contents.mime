vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"if(primintrinsic(0, \"closed\", @primnum)){
    if(chi(\"pin_borders\")){
        
        int h = primhedge(0, @primnum);
        int temp = h;
        
        do{
            if(hedge_equivcount(0,h) <= 1){
                setpointattrib(0, \"__bound_weight\", hedge_srcpoint(0, h), 0);
                setpointattrib(0, \"__bound_weight\", hedge_dstpoint(0, h), 0);
            }
            h = hedge_next(0,h);
        }while(h != temp);
    }
}else{
    int pp[] = primpoints(0, @primnum);
    foreach(int pt; pp){
        if(neighbourcount(0, pt) <= 1){
            setpointattrib(0, \"__bound_weight\", pt, 0);
        }
    }
}    
"	)
