--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/ambient_occlusion.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int mode = chi(\"mode\");

int sample_count = mode < 3 ? npoints(1) : chi(\"sample_count\");
float ray_length = chf(\"ray_length\");
float ray_offset = chf(\"ray_offset\");
float exp = chf(\"exp\");
float ao_mask = 0.0;
float weight = 0.0;

if (mode == 0) {
    // Physical Ambient Occlusion:

    for (int pt = 0; pt < sample_count; pt ++) {

        vector dir = point(1, \"P\", pt);
        vector4 q = dihedral({0, 0, 1}, v@_pao_N);
        dir = qrotate(q, dir);
        float cos_theta = max(0, dot(dir, v@_pao_N));
        vector hit_P, hit_uvw;

        if (chi(\"debug\")) {
            addprim(0, \"polyline\", @ptnum, addpoint(0, @P + dir * ray_length * chf(\"debug_scale\")));
        }

        if (intersect(0, @P + dir * ray_offset, dir * ray_length, hit_P, hit_uvw) != -1) {
            ao_mask += pow(distance(v@_pao_N, hit_P) / ray_length, exp) * cos_theta;
        } else {
            ao_mask += cos_theta;
        }

        weight += cos_theta;
    }

} else if (mode == 3) {
    // Dirt Buildup:

    float seed = chf(\"seed\");
    float half_cone = chf(\"half_cone\");

    for (int pt = 0; pt < sample_count; pt ++) {

        vector2 sample_seed = set(random_brj(seed + @ptnum, pt), random_brj(seed * 2 + @ptnum, pt));
        vector dir = sample_direction_cone(v@_pao_N, radians(half_cone), sample_seed);
        vector hit_P, hit_uvw;

        if (chi(\"debug\")) {
            addprim(0, \"polyline\", @ptnum, addpoint(0, @P + dir * ray_length * chf(\"debug_scale\")));
        }

        if (intersect(0, @P + dir * ray_offset, dir * ray_length, hit_P, hit_uvw) == -1) {
            ao_mask += 1.0;
        }

        weight += 1.0;
    }

} else {
    // Valleys or Peaks:

    if (mode == 2) {
        // Peaks:
        v@_pao_N *= -1.0;
    }

    for (int pt = 0; pt < sample_count; pt ++) {

        vector dir = point(1, \"P\", pt);
        vector4 q = dihedral({0, 0, 1}, v@_pao_N);
        dir = qrotate(q, dir);
        vector hit_P, hit_uvw;

        if (chi(\"debug\")) {
            addprim(0, \"polyline\", @ptnum, addpoint(0, @P + dir * ray_length * chf(\"debug_scale\")));
        }

        if (intersect(0, @P + dir * ray_offset, dir * ray_length, hit_P, hit_uvw) == -1) {
            ao_mask += 1.0;
        }

        weight += 1.0;
    }
}

f@_pao_mask = ao_mask / weight;"	)
