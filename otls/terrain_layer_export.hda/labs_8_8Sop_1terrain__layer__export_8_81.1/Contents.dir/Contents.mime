--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/python1.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"import os

def SaveLayer(a_LayerName):
    
    # Get Export Path
    Path = hou.node(\"..\").parm(\"output\").evalAsString()
    if Path[-1] != \"/\":
        Path = Path + \"/\"
    Path = \"/\".join(Path.split(\"/\")[:-1])


    FileName = hou.node(\"..\").parm(\"sPaintLayerPrefix\").evalAsString() + \"%s.png\" % LayerName
    if LayerName == \"height\":
        FileName = hou.node(\"..\").parm(\"sPaintLayerPrefix\").evalAsString() + \"%s.png\" % hou.node(\"..\").parm(\"sHeightmapName\").evalAsString()
    ExportFile = Path + \"/\" +  FileName


    # Determine HF Output node. Heightmap needs remapping, while paint layers do not.
    HF_Output = hou.node('../heightfield_output_mask')

    if LayerName == \"height\":
        HF_Output = hou.node('../heightfield_output_height')
        HF_Output.parm(\"output\").set(ExportFile)
        #hou.node(\"..\").parm(\"sOutputHeight\").set(ExportFile)
    else:
        HF_Output.parm(\"output\").set(ExportFile)
        hou.node(\"..\").parm(\"sOutputLayer\").set(LayerName)
        #hou.node(\"..\").parm(\"sOutputPaint\").set(ExportFile)
        

    # Render to disk
    HF_Output.parm(\"execute\").pressButton()

    # Debug
    #print \"Printing\", HF_Output.parm(\"output\").evalAsString()

    
# Get File Name
LayerName = hou.pwd().geometry().iterPrims()[0].attribValue(\"name\")

if LayerName == 'height':
    SaveLayer(LayerName)
else:
    if hou.node(\"..\").parm(\"bExportAllLayers\").evalAsInt() == 1:
        SaveLayer(LayerName)
    elif LayerName in hou.pwd().geometry().attribValue(\"exportlayers\").split(\" \"):
        SaveLayer(LayerName)"	)
}

