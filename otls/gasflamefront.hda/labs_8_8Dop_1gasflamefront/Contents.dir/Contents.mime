--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/combustion.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	0	)
snippet	[ 0	locks=0 ]	(	"@burn = 0;

if (@fuel < 0.001)
    @fuel = 0;

if (@flamefront >= 0)
    return;

float oxygen = 1.0 - @fuel - @waste;
if (oxygen < 0)
    return;

// Important flamefront starts at 0, not centered at zero.
float voxelsize = ch(\"voxelsize\");
float ratio = min(-@flamefront / voxelsize, 1);
    
// 1:1 ratio.
@burn = min(oxygen, @fuel);

@burn *= ratio;

@fuel -= @burn;
@waste += @burn * 2;     // Include lost oxygen!

// Flame is 1 for fresh burn, add a bit of taper.
@flame = max(@flame, min(1, @burn * 10));
@temperature += @burn * ch(\"temp_gain\");
@divergence += @burn * ch(\"expansion\");"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
vex_stencil	[ 0	locks=0 ]	(	active	)
vex_precision	[ 0	locks=0 ]	(	auto	)
bindinputmenu1	[ 0	locks=0 ]	(	"none"	)
bindgeoinput1	[ 0	locks=0 ]	(	""	)
binddopinput1	[ 0	locks=0 ]	(	""	)
bindinputmenu2	[ 0	locks=0 ]	(	"none"	)
bindgeoinput2	[ 0	locks=0 ]	(	""	)
binddopinput2	[ 0	locks=0 ]	(	""	)
bindinputmenu3	[ 0	locks=0 ]	(	"none"	)
bindgeoinput3	[ 0	locks=0 ]	(	""	)
binddopinput3	[ 0	locks=0 ]	(	""	)
bindinputmenu4	[ 0	locks=0 ]	(	"none"	)
bindgeoinput4	[ 0	locks=0 ]	(	""	)
binddopinput4	[ 0	locks=0 ]	(	""	)
usetimestep	[ 0	locks=0 ]	(	"on"	)
timescale	[ 0	locks=0 ]	(	1	)
folder1	[ 0	locks=0 ]	(	0	0	0	)
temp_gain	[ 0	locks=0 ]	(	[ temp_gain	3 ] 	)
expansion	[ 0	locks=0 ]	(	[ expansion	0 ] 	)
voxelsize	[ 0	locks=0 ]	(	[ voxelsize	0.02 ] 	)
}

