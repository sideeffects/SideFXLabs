vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
targetpolycount	[ 0	locks=0 ]	(	[ targetpolycount	0 ] 	)
Multiplier_Toggle	[ 8	locks=0 ]	(	[ Multiplier_Toggle	0 ] 	)
Multiplier	[ 8	locks=0 ]	(	[ Multiplier	0 ] 	)
spare_input0	[ 0	locks=0 ]	(	../IN_Geo	)
