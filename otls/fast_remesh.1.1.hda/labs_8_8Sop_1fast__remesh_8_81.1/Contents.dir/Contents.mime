--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/find_undivided_prims.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_frm_unsharededges	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"//Using the unshared edges group, we find neighboring primitives of an unfused point that the point
//does not lie on, and we perform an xyzdist function to see which primitive this point is touching
//to then subdivide that prim later (using the location of the touching point)


vector uv;

int prims[] = pointprims(0, @ptnum);
int neighbors[] = neighbours(0, @ptnum);
int neighbor_prims[];
vector neighbor_pos[];

//Find neighboring points to return resulting pointprims (of just the neighbors)
foreach(int nbr; neighbors)
{
    int nbrprims[] = pointprims(0, nbr);
    append(neighbor_prims, nbrprims);
}

foreach(int prim; prims)
{
    foreach(int nbr; neighbor_prims)
    {
        if(nbr == prim)
        {
           removevalue(neighbor_prims, nbr);
        }
    }
}


//Create a string group to feed into the xyzdist function, so we only check against these prims
//and not every prim in the mesh (which would slow down calculation considerably)
string neighborgrp = \"\";

foreach(int nbr; neighbor_prims)
{
    neighborgrp += itoa(nbr) + \" \";
}

xyzdist(0, neighborgrp, v@P, i@_frm_hitprim, uv, 0.0001);

//Add the list of touching points (_frm_foundpts) that intersected a primitive to the primitive itself, and add these primitives
//to a hit group
setprimattrib(0, \"_frm_foundpts\", i@_frm_hitprim, @ptnum, \"append\");
setprimgroup(0, \"_frm_hitprims\", i@_frm_hitprim, 1, \"set\");

"	)
exportlist	[ 0	locks=0 ]	(	*	)
