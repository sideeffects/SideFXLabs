--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/create_new_prims_and_preserve_uvs.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_frm_hitprims	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"//Using the apex point, the touching point and other two original points of the triangle
//We create two triangles from the original triangle primitive
int primpts[] = primpoints(0, @primnum);

foreach(int pt; primpts)
{
    if(pt == i@_frm_apexpt)
    {
        removevalue(primpts, pt);
    }
}

//Create the two triangle primitives
for(int i = 0; i < 2; i++)
{
    
    int prim = addprim(0, \"poly\");
    
    //Here we check to make sure the winding order is correct with point order. If not, we reverse it
    int points[] = array(primpts[i], i@_frm_apexpt, i[]@_frm_foundpts[0]);
    

    vector pos_a = point(0, \"P\", points[0]);
    vector pos_b = point(0, \"P\", points[1]);
    vector pos_c = point(0, \"P\", points[2]);
    
    vector ab = pos_b - pos_a;
    vector bc = pos_c - pos_b;
    
    vector cross = cross(ab, bc);
    
    float direction = dot(v@_frm_prim_N, cross);
    if(direction > 0)
    {
        points = reverse(points);
    }

    
    string neighborgrp = itoa(@primnum);
    vector uv;
    int prim_hold;
    
    
    foreach(int pt; points)
    {   
        //Add vertices to new shape
        int v = addvertex(0, prim, pt);
        vector pos = point(0, \"P\", pt);
        
        //Samples the primitive being adjusted at the creation points
        //for the uv value to keep uvs intact
        xyzdist(0, neighborgrp, pos, prim_hold, uv, 0.001);
        vector new_uv = primuv(0, \"uv\", @primnum, uv);
        setvertexattrib(0, \"uv\", v, -1, new_uv, \"set\");
    }
    
    
    setprimattrib(0, \"shop_materialpath\", prim, s@shop_materialpath, \"set\");
    setprimattrib(0, \"_frm_remeshworkitem\", prim, @_frm_remeshworkitem, \"set\");
    setprimattrib(0, \"_frm_prim_N\", prim, v@_frm_prim_N, \"set\");
    setprimgroup(0, \"_frm_newprims\", prim, 1, \"set\");
    
    
}

removeprim(0, @primnum, 1);
removevalue(i[]@_frm_foundpts, i[]@_frm_foundpts[0]);
setprimgroup(0, \"_frm_hitprims\", @primnum, 0, \"set\");


"	)
