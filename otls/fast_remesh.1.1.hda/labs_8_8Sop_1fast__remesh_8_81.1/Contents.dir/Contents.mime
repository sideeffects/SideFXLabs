--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/find_apex_pts.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_frm_hitprims	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int primpts[] = primpoints(0, @primnum);
vector pt_pos[];

foreach(int pt; primpts)
{
    vector pos = point(0, \"P\", pt);
    append(pt_pos, pos);
}

vector found_pos = point(0, \"P\", i[]@_frm_foundpts[0]);

/*Find which edge of the triangle the touching points (_frm_foundpts) rest on by 
finding the angle between the vectors formed by the first touching point and the prim points.
The touching point will have an angle of pi (180 degrees) with the points it shares a line with,
and the apex point will create an angle less than this value, which is how we identify the apex point,
the point that will connect with the touching point to create a new edge.*/



float angles[];
for(int i = 0; i < 3; i++)
{
    
    vector a = found_pos - pt_pos[i];
    vector b;
    
    if(i == 2)
    {
       b = found_pos - pt_pos[0];
    }
    
    else 
    {
        b = found_pos - pt_pos[i + 1];
    }
    
    float mag_a = length(a);
    float mag_b = length(b);
    
    float angle = abs(acos((dot(a, b) / (mag_a * mag_b))));
    append(angles, angle);

}

float max_angle = max(angles);
for(int i = 0; i < 3; i++)
{
    if(angles[i] == max_angle)
    {
        if(i == 0)
        {
            i@_frm_apexpt = primpts[2];
        }
        
        else
        {
            i@_frm_apexpt = primpts[i - 1];
        }
    }
}

"	)
