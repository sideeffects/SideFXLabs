generate	[ 0	locks=0 ]	(	"# Called when this node should generate new work items from upstream items.
#
# self             -   A reference to the current pdg.Node instance
# item_holder      -   A pdg.WorkItemHolder for constructing and adding work items
# upstream_items   -   The list of work items in the node above, or empty list if there are no inputs
# generation_type  -   The type of generation, e.g. pdg.generationType.Static, Dynamic, or Regenerate

import hou
node = hou.pwd()

filter_timedependent = node.evalParm(\"filtertimedependent\")
filter_timeindependent = node.evalParm(\"filtertimeindependent\")
filter_wedges = node.evalParm(\"filterwedge\")
filter_direction = node.evalParm(\"filteroperation\")
filter_subframes = node.evalParm(\"filtersubframes\")
filter_attrib = node.evalParm(\"filterattrib\")

# FUNCTIONS
def isGeneric(work_item):
    if work_item.hasFrame:
        return False
    return True

def isWedged(work_item):
    if work_item.attrib(\"wedgeindex\") and work_item.attrib(\"wedgenum\"):
        return True
    return False
    
def isTimeDependent(work_item):
    '''
        Checks the type of work item. Returns None if the work item has no frame attribute.
        If it has, checks if the range and frame is not 1 (Time Dependent), otherwise
        it is Time Independent
    '''
   
    if isGeneric(work_item):
        return False
        
    frame = work_item.frame
    range = work_item.floatAttribArray('range') 
    if range[0] == range[1] == range[2] == frame == 1:
        return False
    else:
        return True
      
def isSubframe(work_item):
    
    if isGeneric(work_item):
        return False
        
    frame = work_item.frame
    range = work_item.floatAttribArray('range') 
    if range[0] == range[1] == range[2] == frame == 1:
        return False
    else:
        return not frame.is_integer()

def getRangeValues():

    start = end = stepsize = 1    
    frame_pattern = \"{}-{}:{}\".format(start, end, stepsize)
    range_mode = node.parm(\"trange\").evalAsInt()
    
    if range_mode == 0:
        return \"all\"
    elif range_mode == 1:
        if node.evalParm(\"useframeoverride\"):
            start = end = node.evalParm(\"frameoverride\")            
        else:
            start = end = node.evalParm(\"frame\")  
        frame_pattern = \"{}-{}:{}\".format(start, end, stepsize)        
    elif range_mode == 2:
        start = node.evalParm(\"f1\")
        end = node.evalParm(\"f2\")
        stepsize = node.evalParm(\"f3\")
        frame_pattern = \"{}-{}:{}\".format(start, end, stepsize)
    else:
        frame_pattern = node.evalParm(\"specificrange\")   
        
    return pdg.ValuePattern(frame_pattern).floatArray(inclusive=True)

def getWedgeValues():

    start = end = stepsize = 1
    wedge_pattern = \"{}-{}:{}\".format(start, end, stepsize)
    wedge_mode = node.parm(\"wrange\").evalAsInt()
    
    if wedge_mode == 0:
        return \"all\"
    elif wedge_mode == 1:        
        start = end = node.evalParm(\"wedgeoverride\")
        wedge_pattern = \"{}-{}:{}\".format(start, end, stepsize) 
    elif wedge_mode == 2:
        start = node.evalParm(\"wedgerangemin\")
        end = node.evalParm(\"wedgerangemax\")
        wedge_pattern = \"{}-{}:{}\".format(start, end, stepsize)    
    else:
        wedge_pattern = node.evalParm(\"specificwedges\") 

    return pdg.ValuePattern(wedge_pattern).intArray(inclusive=True)    

# MAIN 

range_values = getRangeValues()
wedge_values = getWedgeValues()

for work_item in upstream_items: 

    filter = False
    
    if isGeneric(work_item):
        filter = False
   
    if filter_wedges and not (filter_timeindependent or filter_timedependent):     
        if wedge_values == \"all\":
            filter = True
        else:
            if work_item.intAttribValue(\"wedgenum\") in wedge_values:
                filter = True
    else:
        if isTimeDependent(work_item):
            if filter_timedependent:
                if range_values == \"all\":
                    filter = True
                else:
                    if work_item.frame in range_values:
                        filter = True
            if filter_wedges:
                if filter == True:
                    if wedge_values == \"all\":
                        filter = True
                    else:
                        if work_item.intAttribValue(\"wedgenum\") not in wedge_values:       
                            filter = False
        else:
            if filter_timeindependent:
                filter = True
            if filter_wedges:
                if filter == True:
                    if wedge_values == \"all\":
                        filter = True
                    else:
                        if work_item.intAttribValue(\"wedgenum\") not in wedge_values:
                            filter = False      
            
    if not filter_direction:
        filter = not filter
        
    if filter_subframes and isSubframe(work_item):
        filter = False
    
    #print(\"{}-{} : {}\".format(int(work_item.frame),work_item.intAttribValue(\"wedgenum\"),filter))
    new_item = item_holder.addWorkItem(parent=work_item)
    new_item.setIntAttrib(filter_attrib, int(filter))"	)
useregeneratestatic	[ 0	locks=0 ]	(	"off"	)
