Content-Disposition: attachment; filename="hdaroot/python_filter1.spareparmdef"
Content-Type: text/plain

    parm {
        name    "savenodescript"
        baseparm
        label   "Save to Python Script"
        joinnext
        export  none
    }
    parm {
        name    "savenodehda"
        baseparm
        label   "Save to Digital Asset"
        export  none
    }
    parm {
        name    "sepparm2"
        baseparm
        label   "Separator"
        export  none
    }
    group {
        name    "folder0"
        label   "Filter"

        parm {
            name    "filteroperation"
            label   "Filter Non Selected"
            type    toggle
            default { "1" }
            disablewhen "{ filtertimedependent != 1 }"
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "filtersubframes"
            label   "Filter Subframes"
            type    toggle
            default { "0" }
            disablewhen "{ filtertimedependent != 1 }"
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "filterattrib"
            label   "Filter Attribute"
            type    string
            default { "filter" }
            parmtag { "script_callback_language" "python" }
        }
        groupcollapsible {
            name    "folder3"
            label   "Time Independent"
            grouptag { "sidefx::header_toggle" "filtertimeindependent" }

            parm {
                name    "filtertimeindependent"
                label   "Filter Time Independent"
                type    toggle
                default { "1" }
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "script_callback_language" "python" }
            }
        }

        groupcollapsible {
            name    "folder1"
            label   "Time Dependent"
            grouptag { "sidefx::header_parm" "trange" }
            grouptag { "sidefx::header_toggle" "filtertimedependent" }

            parm {
                name    "filtertimedependent"
                label   "Filter by Range"
                type    toggle
                default { "1" }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "trange"
                label   "Filter by"
                type    ordinal
                default { "all" }
                disablewhen "{ filtertimedependent != 1 }"
                menu {
                    "all"       "All Frames"
                    "single"    "Single Frame"
                    "range"     "Frame Range"
                    "specific"  "List of Frames"
                }
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "useframeoverride"
                label   "Override Frame"
                type    toggle
                nolabel
                joinnext
                default { "off" }
                disablewhen "{ filtertimedependent != 1 }"
                hidewhen "{ trange != single }"
                parmtag { "autoscope" "0000000000000000" }
            }
            parm {
                name    "frameoverride"
                label   "Override Frame"
                type    float
                default { "1" }
                disablewhen "{ filtertimedependent != 1 } { useframeoverride == 0 }"
                hidewhen "{ trange != single }"
                range   { 1 240 }
                parmtag { "autoscope" "0000000000000000" }
            }
            parm {
                name    "frame"
                label   "Frame"
                type    float
                default { "$FF" }
                disablewhen "{ filtertimedependent != 1 } { useframeoverride == 1 }"
                hidewhen "{ trange != single }"
                range   { 0 240 }
                parmtag { "autoscope" "0000000000000000" }
            }
            parm {
                name    "f"
                label   "Frame Range"
                type    float
                size    3
                default { "$FSTART" "$FEND" "1" }
                disablewhen "{ filtertimedependent != 1 }"
                hidewhen "{ trange != range }"
                range   { 1 240 }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "specificrange"
                label   "List of Frames"
                type    string
                default { "`chs(\"f1\")`-`chs(\"f2\")`:`chs(\"f3\")`" }
                disablewhen "{ filtertimedependent != 1 }"
                hidewhen "{ trange != specific }"
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "script_callback_language" "python" }
            }
        }

        groupcollapsible {
            name    "folder2"
            label   "Wedges"
            grouptag { "sidefx::header_parm" "wrange" }
            grouptag { "sidefx::header_toggle" "filterwedge" }

            parm {
                name    "filterwedge"
                label   "Filter by Wedge"
                type    toggle
                default { "1" }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "wrange"
                label   "Filter by"
                type    ordinal
                default { "all" }
                disablewhen "{ filterwedge != 1 }"
                menu {
                    "all"       "All Wedges"
                    "single"    "Single Wedge"
                    "range"     "Custom Range"
                    "specific"  "List of Wedges"
                }
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "script_callback" "" }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "wedgeoverride"
                label   "Single Wedge"
                type    integer
                default { "0" }
                disablewhen "{ filterwedge != 1 }"
                hidewhen "{ wrange != single }"
                range   { 0! 49 }
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "sidefx::slider" "snap_to_int" }
            }
            parm {
                name    "wedgerange"
                label   "Wedge Range"
                type    int_minmax
                size    2
                default { "0" "1" }
                disablewhen "{ filterwedge != 1 }"
                hidewhen "{ wrange != range }"
                range   { 0! 100 }
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "specificwedges"
                label   "List of Wedges"
                type    string
                default { "0-1" }
                disablewhen "{ filterwedge != 1 }"
                hidewhen "{ wrange != specific }"
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "script_callback_language" "python" }
            }
        }

    }

    group {
        name    "folder0_1"
        label   "Processor"

        parm {
            name    "pdg_workitemgeneration"
            baseparm
            label   "Generate When"
            export  none
        }
        parm {
            name    "pdg_command"
            baseparm
            label   "Command"
            export  none
        }
        parm {
            name    "sepparm"
            baseparm
            label   "Separator"
            export  none
        }
        group {
            name    "callbacks6"
            label   "Generate"

            parm {
