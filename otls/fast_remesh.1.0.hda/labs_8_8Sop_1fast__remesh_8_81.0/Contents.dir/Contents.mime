Content-Disposition: attachment; filename="hdaroot/calculate_average_edge_ln_from_input.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"float avg_sum = 0.0;

for(int i = 0; i < nprimitives(0); i++)
{
    int pts[] = primpoints(0, i);
    float edges[]; // store array of edge lengths
    
    int j = 0;
    vector pos0, pos1;
    
    foreach(int pt; pts)
    {
        pos0 = point(0, \"P\", pt);
        if(pt == pts[len(pts) - 1]) // if it is last pt
        {
            pos1 = point(0, \"P\", pts[0]);   
        }
        
        else // we want pos of the next pt on prim
        {
            pos1 = point(0, \"P\", pts[j + 1]);
        }
        
        j += 1;
        float edge = distance(pos0, pos1);
        append(edges, edge);
    }
    
    float sum = 0;
    foreach(float e; edges)
    {
        sum += e ;
    }
    
    avg_sum += sum/len(edges);
    
}

f@_frm_avg_edgelength = avg_sum/nprimitives(0);
"	)
