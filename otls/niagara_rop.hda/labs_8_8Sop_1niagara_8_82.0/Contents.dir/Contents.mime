Content-Type: text/plain

    parm {
        name    "execute"
        baseparm
        label   "Render"
        joinnext
        export  none
    }
    parm {
        name    "renderdialog"
        baseparm
        label   "Controls..."
        nolabel
        export  none
    }
    group {
        name    "fd_general3"
        label   "General"

        groupsimple {
            name    "fd_ropsettings2"
            label   "ROP Settings"

            parm {
                name    "trange"
                baseparm
                label   "Valid Frame Range"
                invisible
                export  none
            }
            parm {
                name    "range_mode"
                baseparm
                label   "Frame Range / Time Mode"
                export  none
            }
            parm {
                name    "f"
                baseparm
                label   "Start/End/Inc"
                export  none
            }
            parm {
                name    "take"
                baseparm
                label   "Render with Take"
                export  none
            }
            parm {
                name    "soppath"
                baseparm
                label   "SOP to Export"
                export  none
            }
            parm {
                name    "mkpath"
                baseparm
                label   "Create Intermediate Directories"
                export  none
            }
            parm {
                name    "outputpath"
                baseparm
                label   "Output Path"
                export  none
            }
        }

        groupsimple {
            name    "fd_attributes2"
            label   "Attributes"

            multiparm {
                name    "fd_numofrenames"
                label    "Number of Renames"
                baseparm
                default 0
                parmtag { "autoscope" "0000000000000000" }
                parmtag { "multistartoffset" "0" }

                parm {
                    name    "frompt#"
                    baseparm
                    label   "Attribute From/To"
                    joinnext
                    export  none
                }
                parm {
                    name    "topt#"
                    baseparm
                    label   "To Attribute"
                    nolabel
                    export  none
                }
            }

            parm {
                name    "overwrite"
                baseparm
                label   "Overwrite Existing Attributes"
                export  none
            }
            parm {
                name    "ptkeep"
                baseparm
                label   "Keep Attributes"
                export  none
            }
            multiparm {
                name    "fd_castattributes"
                label    "Cast Attributes"
                baseparm

                parm {
                    name    "attr_to_cast#"
                    baseparm
                    label   "Attribute"
                    export  none
                }
                parm {
                    name    "attr_type#"
                    baseparm
                    label   "Type"
                    export  none
                }
            }

            parm {
                name    "attr_clamp"
                baseparm
                label   "Clamp Attributes That Are Outside Type Range"
                export  none
            }
        }

    }

    group {
        name    "fd_general3_1"
        label   "Advanced"

        groupsimple {
            name    "fd_timesettings2"
            label   "Time Settings"

            parm {
                name    "export_current_frame_as_time_zero"
                baseparm
                label   "Export Current Frame As time == 0"
                export  none
            }
            parm {
                name    "export_start_frame_as_time_zero"
                baseparm
                label   "Export Start Frame As time == 0"
                export  none
            }
        }

    }

    parm {
        name    "ptrenames"
        label   "ptrenames"
        type    float
        default { "0" }
        range   { 0 10 }
    }
    parm {
        name    "attr_types"
        label   "attr_types"
        type    float
        default { "0" }
        range   { 0 10 }
    }

