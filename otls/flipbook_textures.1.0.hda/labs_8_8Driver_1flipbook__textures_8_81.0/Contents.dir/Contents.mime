code	[ 0	locks=0 ]	(	"// Clamps to range
float r = clamp(R, -1, 1);
float g = clamp(G, -1, 1) * up_sign;
float b = clamp(B, -1, 1);

// Handles edge cases
vector n = set(r, g, b);
n = n != {0, 0, 0} ? normalize(n) : {0, 0, 1};
n = lerp({0, 0, 1}, n, A);

// Remaps to output range
R = n.x * 0.5 + 0.5;
G = n.y * 0.5 + 0.5;
B = n.z * 0.5 + 0.5;"	)
