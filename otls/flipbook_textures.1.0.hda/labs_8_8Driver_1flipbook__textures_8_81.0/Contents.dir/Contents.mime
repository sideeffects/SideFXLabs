{
version 0.8
code	[ 0	locks=0 ]	(	"// Clamps to range
float r = clamp(R, 0, 1);
float g = clamp(G, 0, 1);
float b = clamp(B, 0, 1);
float a = clamp(A, 0, 1);

// Constructs RG
r = clamp(r - b, -1, 1);
g = clamp(g - a, -1, 1) * up_sign;
r *= intensity;
g *= intensity;

// Derives B
b = sqrt(clamp(1.0 - r * r - g * g, 0, 1));

// Handles edge cases
vector n = set(r, g, b);
n = n != {0, 0, 0} ? normalize(n) : {0, 0, 1};

// Remaps to output range
R = n.x * 0.5 + 0.5;
G = n.y * 0.5 + 0.5;
B = n.z * 0.5 + 0.5;"	)
