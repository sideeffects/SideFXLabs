--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/motion_vector/geo/mv_from_delta_P.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int clamp_mode = chi(\"clamp_mode\");
int pt_track_method = chi(\"pt_track_method\");
float time_inc = @TimeInc * max(chi(\"f3\"), 1);
vector mv = {0.0, 0.0, 0.0};


// Attempts to establish a valid next-frame point or previous-frame point.
// At most, only one of them will be valid, which then indicates the tracking direction.

int next_frame_pt = -1;
int prev_frame_pt = -1;
vector next_frame_pos = @Cd;
vector prev_frame_pos = @Cd;

int must_track_backwards = clamp_mode == 0 && @Frame == chi(\"f2\");

if (pt_track_method == 0 && detail(0, \"__has_id\", 0) && @id != -1) {
    // Attempts to track by ID (@name):

    // If this fails, tracking by point number will be skipped
    // because that method is unsafe for points with valid IDs.

    if (!must_track_backwards) {
        next_frame_pt = nametopoint(1, @name);
    }

    if (next_frame_pt == -1) {
        prev_frame_pt = nametopoint(2, @name);
    }

} else if (pt_track_method == 2) {
    // Tracks by proximity:

    // This is very unlikely to fail unless there is zero point
    // on both adjacent frames, in which case falling back to
    // tracking by point number will also safely fail.

    if (!must_track_backwards) {
        next_frame_pt = nearpoint(1, @P);
    }

    if (next_frame_pt == -1) {
        prev_frame_pt = nearpoint(2, @P);
    }
}


if (next_frame_pt != -1) {

    next_frame_pos = point(1, \"Cd\", next_frame_pt);
    mv = (next_frame_pos - @Cd) / time_inc;

} else if (prev_frame_pt != -1) {

    prev_frame_pos = point(2, \"Cd\", prev_frame_pt);
    mv = (@Cd - prev_frame_pos) / time_inc;

} else if (@id == -1) {
    // Attempts to track by point number only if
    // the point doesn't have a valid ID:

    if (!must_track_backwards) {

        if (@ptnum < npoints(1)) {

            next_frame_pt = @ptnum;
            next_frame_pos = point(1, \"Cd\", next_frame_pt);
            mv = (next_frame_pos - @Cd) / time_inc;
        }
    }

    if (next_frame_pt == -1) {

        if (@ptnum < npoints(2)) {

            prev_frame_pt = @ptnum;
            prev_frame_pos = point(2, \"Cd\", prev_frame_pt);
            mv = (@Cd - prev_frame_pos) / time_inc;
        }
    }
}


// toDNC()'s range is [0, 1],
// instead of volumetric motion vectors' [-1, 1] range
@Cd = mv * 2.0;"	)
