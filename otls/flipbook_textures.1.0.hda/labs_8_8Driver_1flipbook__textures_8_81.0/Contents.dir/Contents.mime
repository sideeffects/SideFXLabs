--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/comp_normal/normal_blending/snippet1.parm"
Content-Type: text/plain

{
version 0.8
code	[ 0	locks=0 ]	(	"// Clamps MDC normal to range
float r = clamp(R, 0, 1);
float g = clamp(G, 0, 1);
float b = clamp(B, 0, 1);
float a = clamp(A, 0, 1);

// Constructs MDC normal RG
r = clamp(r - b, -1, 1);
g = clamp(g - a, -1, 1) * up_sign;
r *= intensity;
g *= intensity;

// Derives MDC normal B
b = sqrt(clamp(1.0 - r * r - g * g, 0, 1));

// Handles MDC normal edge cases
vector n = set(r, g, b);
n = n != {0, 0, 0} ? normalize(n) : {0, 0, 1};

// Resets MDC normal rgb
r = n.x;
g = n.y;
b = n.z;

// Clamps surface normal to range
float r1 = clamp(sn.x, -1, 1);
float g1 = clamp(sn.y, -1, 1) * up_sign;
float b1 = clamp(sn.z, -1, 1);

// Handles surface edge cases
n = set(r1, g1, b1);
n = n != {0, 0, 0} ? normalize(n) : {0, 0, 1};
n = lerp({0, 0, 1}, n, sn_alpha);

// Blends MDC and surface normals
r = lerp(r, n.x, sn_weight);
g = lerp(g, n.y, sn_weight);
b = lerp(b, n.z, sn_weight);

// Handles final normal edge cases
n = set(r, g, b);
n = n != {0, 0, 0} ? normalize(n) : {0, 0, 1};

// Remaps to output range
R = n.x * 0.5 + 0.5;
G = n.y * 0.5 + 0.5;
B = n.z * 0.5 + 0.5;"	)
