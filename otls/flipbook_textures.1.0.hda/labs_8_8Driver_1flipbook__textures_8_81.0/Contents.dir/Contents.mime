{
version 0.8
code	[ 0	locks=0 ]	(	"float pointing_up = max(sign(G), 0);
float length = length(set(R, G));

float angle = acos(dot(normalize(set(R, G, 0)), {1, 0, 0})) / 6.28318530718;

angle = pointing_up * angle + (1.0 - pointing_up) * (1.0 - angle);
angle *= 511.0;

R = angle % 256.0 / 255.0;

float angle_encoding_offset = floor(angle / 256.0) * 128.0;

G = (length * 127.0 + angle_encoding_offset) / 255.0;

if (length < 0.001) {

    R = 0.0;
    G = 0.0;
}"	)
