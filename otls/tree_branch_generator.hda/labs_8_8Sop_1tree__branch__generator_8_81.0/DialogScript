            default { "1e-05" }
            disablewhen "{ enable_disp == 0 } { disp_reduce_toggle == 0 } { disp_override == 0 check_attrib == 1 }"
            hidewhen "{ enable_disp == 0 } { disp_reduce_toggle == 0 }"
            range   { 0! 1 }
            parmtag { "autoscope" "0000000000000000" }
            parmtag { "script_callback_language" "python" }
        }
    }

    group {
        name    "fd_general_7"
        label   "Visualization"

        parm {
            name    "vis_override"
            label   "Override Visualization"
            type    toggle
            default { "0" }
            hidewhen "{ check_attrib == 0 }"
            parmtag { "autoscope" "0000000000000000" }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "sepparm23"
            label   "sepparm23"
            type    separator
            default { "" }
            hidewhen "{ check_attrib == 0 }"
        }
        parm {
            name    "vis_toggle"
            label   "Enable Visualization Color"
            type    toggle
            default { "0" }
            disablewhen "{ vis_override == 0 check_attrib == 1 }"
            parmtag { "autoscope" "0000000000000000" }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "color"
            label   "Color"
            type    color
            size    3
            default { "1" "1" "1" }
            disablewhen "{ vis_toggle == 0 } { vis_override == 0 check_attrib == 1 }"
            range   { 0 1 }
            parmtag { "autoscope" "0000000000000000" }
        }
    }

    group {
        name    "fd_general_8"
        label   "Handle Controls"
        invisibletab

        parm {
            name    "handlelength"
            label   "handlelength"
            type    float
            default { "1" }
            range   { 0 10 }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "vector"
            label   "vector"
            type    vector
            size    3
            default { "0" "1" "0" }
            range   { -1 1 }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "enable"
            label   "enable"
            type    toggle
            default { "1" }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "ty"
            label   "ty"
            type    float
            default { "10" }
            range   { 0 10 }
            parmtag { "script_callback_language" "python" }
        }
    }

