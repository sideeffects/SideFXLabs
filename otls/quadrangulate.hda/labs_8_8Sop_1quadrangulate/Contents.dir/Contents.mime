{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int point, point_next, point_prev;
int num = primvertexcount(0, @primnum);
if(num == 3)
{
    for(int i = 0; i < num; i++)
    {
        point = vertexpoint(0, vertexindex(0, @primnum, i));
        point_next = vertexpoint(0, vertexindex(0, @primnum, (i + 1) % num));
        point_prev = vertexpoint(0, vertexindex(0, @primnum, (i - 1) % num));
    
        float edge_1 =  length(vector(point(0, \"P\", point_prev) - point(0, \"P\", point)));
        float edge_2 =  length(vector(point(0, \"P\", point_next) - point(0, \"P\", point)));
        float edge_3 =  length(vector(point(0, \"P\", point_next) - point(0, \"P\", point_prev)));
        
        float max_edge = max(max(edge_1, edge_2), edge_3);
        
        if (edge_1 == max_edge)
        {

            setedgegroup(0, \"long_edge\", point_prev, point, 1);
            
        }
        if (edge_2 == max_edge)
        {
             setedgegroup(0, \"long_edge\", point_next, point, 1);
        }
        if (edge_3 == max_edge)
        {
            setedgegroup(0, \"long_edge\", point_next, point_prev, 1);
        }
    }

}



"	)
