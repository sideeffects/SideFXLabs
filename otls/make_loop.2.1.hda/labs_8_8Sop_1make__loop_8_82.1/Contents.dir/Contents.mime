--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/input_adjustment.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"float range_start, range_end, birth_time, death_time;

// @Time starts from @Frame=1.
range_start = @TimeInc * (chi(\"start\") - 1);
range_end = @TimeInc * (chi(\"end\") - 1);

birth_time = @Time - @age;
death_time = birth_time + @life;


// Clamps birth to 'range_start'
if (birth_time < range_start) {

    if (chi(\"bornbeforestartmode\") == 0) {

        removepoint(0, @ptnum);
        return;

    } else {

        @age = @Time - range_start;
        birth_time = range_start;
    }
}

// Clamps death to 'range_end'
if (death_time > range_end) {

    if (chi(\"deadafterendmode\") == 0) {

        removepoint(0, @ptnum);
        return;

    } else {

        @life = range_end - birth_time;
    }
}


if (chi(\"fade_in\")) {

    float duration = chf(\"fadein_duration\");

    if (@age <= duration) {

        @pscale *= @age / duration;
        @pscale = max(0, @pscale);
    }
}


if (chi(\"fade_out\")) {

    float duration = chf(\"fadeout_duration\");

    if (@life - @age <= duration) {

        @pscale *= (@life - @age) / duration;
        @pscale = max(0, @pscale);
    }
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
