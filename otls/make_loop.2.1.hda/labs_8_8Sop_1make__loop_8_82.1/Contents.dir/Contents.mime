folder1	[ 0	locks=0 ]	(	0	0	)
start	[ 0	locks=0 ]	(	[ start	0 ] 	)
end	[ 0	locks=0 ]	(	[ end	0 ] 	)
bornbeforestartmode	[ 0	locks=0 ]	(	[ bornbeforestartmode	1 ] 	)
deadafterendmode	[ 0	locks=0 ]	(	[ deadafterendmode	1 ] 	)
fade_in	[ 0	locks=0 ]	(	[ fade_in	0 ] 	)
