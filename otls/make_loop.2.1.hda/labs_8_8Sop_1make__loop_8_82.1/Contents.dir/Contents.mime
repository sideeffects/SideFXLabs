vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int ptnum_at_end = findattribval(1, \"point\", \"id\", @id, 0);

if (ptnum_at_end != -1) {
    // The point is in the group to die:

    float last_frame_time = @TimeInc * (chf(\"endframe\") - 1.0);
    float life_start = @Time - @age;
    
    // DISABLED: helps to greatly reduce the number of glitched points
    // before the first frame of the new loop, but unfortunately also
    // deletes some good points near the end of the previous loop.
    //life_start = rint(life_start / @TimeInc) * @TimeInc;

    @life = last_frame_time - life_start;
    
    // DISABLED: for the same reason stated above
    //@life -= @life % @TimeInc;

    if (@life <= 0.0) {
        removepoint(0, @ptnum);
    }
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
