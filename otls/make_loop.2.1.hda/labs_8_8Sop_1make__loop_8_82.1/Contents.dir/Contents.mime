snippet	[ 0	locks=0 ]	(	"int loop_length = chi(\"looplength\");
int start_frame = chi(\"startframe\");
int base_loops_last_index = chi(\"basebranchloops\");
int max_id_in_one_loop = detail(1, \"__max_id\", 0);

if (@Frame - start_frame < chi(\"shiftlength\")) {

    int loop_index = base_loops_last_index * 2 - 1 + chi(\"dieandrespawn\");
    @id += loop_index * max_id_in_one_loop;
    i@__id_unique = @id - chi(\"dieandrespawn\") * max_id_in_one_loop;

} else {

    int loop_index = base_loops_last_index;
    loop_index += int(floor((@Frame - start_frame - chi(\"shiftlength\")) / loop_length));
    @id += loop_index * max_id_in_one_loop;
    i@__id_unique = @id;
}"	)
