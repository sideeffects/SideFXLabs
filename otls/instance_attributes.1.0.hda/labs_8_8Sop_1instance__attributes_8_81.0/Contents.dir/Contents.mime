snippet	[ 0	locks=0 ]	(	"float NEAR_ZERO = 0.0001;
@N = normalize(@N);
@up = normalize(@up);

// Prevents collinear @N and @up
if (1.0 - abs(dot(@N, @up)) < NEAR_ZERO) {
    @up = set(@N.z, @N.x, -@N.y);
}

int reorient = chi(\"reorient\");
matrix3 m;

if (reorient == 0) {
    m = maketransform(@N, @up);
    
} else if (reorient == 1) {
    m = maketransform(@up, @N);
    
} else {
    m = maketransform(-@up, @N);
}

@orient = quaternion(m);"	)
