--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/rizom_start.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0	autoscope=0 ]	(	"import tempfile, os, subprocess, platform
node        = hou.pwd()
geo         = node.geometry()

rizomPath   = hou.evalParm(\"../rizomloc\")
exportFile  = hou.evalParm(\"../templocation\")
clearUVs    = hou.evalParm(\"../clearUVs\")

useGrp      = hou.evalParm(\"../gtype\")
grpNames    = hou.evalParm(\"../availGroups\")

enableLua   = hou.evalParm(\"../enableLua\")
luaString   = hou.evalParm(\"../lua\")
autoLoad    = hou.evalParm(\"../autoLoad\")
autoSave    = hou.evalParm(\"../autoSave\")
autoQuit    = hou.evalParm(\"../autoQuit\")


def getPointSelection(points):
    zomSelect = \"\"
    for point in points:
        zomSelect += str(point.number()) + \", \"
    if zomSelect != \"\":
        return 'ZomSelect({PrimType=\"Vertex\", WorkingSet=\"Visible\", ResetBefore=true, Select=true, IDs={ ' + zomSelect[:-2] + '}, List=true})'
    else:
        return \"\"       

def getPrimSelection(polygons):
    zomSelect = \"\"
    for prim in polygons:
        zomSelect += str(prim.number()) + \", \"
    if zomSelect != \"\":
        return 'ZomSelect({PrimType=\"Polygon\", WorkingSet=\"Visible\", ResetBefore=true, Select=true, IDs={ ' + zomSelect[:-2] + '}, List=true})'
    else:
        return \"\"

def getEdgeSelection(edges):
    zomSelect = \"\"
    for edge in edges:
        prims = edge.prims()
        pointIDs = []
        points = edge.points()
        #get edge points
        for point in points:
            pointIDs.append(point.number())
        for prim in prims:
            primPTS = []
            for pt in prim.points()[::-1]:
                primPTS.append(pt.number())
            location = 0
            idx1 = primPTS.index(pointIDs[0])
            idx2 = primPTS.index(pointIDs[1])
            
            if idx2 == idx1 + 1:
                location = idx1
            elif idx2 == idx1 - 1:
                location = idx2
            else:
                location = len(prim.points())-1
            zomSelect += str(prim.number()) + \", \" + str(location) + \", \"

    if zomSelect != \"\":
        return 'ZomSelect({PrimType=\"Edge\", WorkingSet=\"Visible\", ResetBefore=true, Select=true, IDs={ ' + zomSelect[:-2] + '}, List=true,  EdgesAsPolyEdgeIDs=true})'
    else: 
        return \"\"
        
def automaticRoundtrip():
    luascript = '''
loadString
selectionString
luaString
saveString
quitString
'''
    
    node.node(\"../rop_fbx1\").parm(\"execute\").pressButton()
    
    script = luascript

    selString = \"\"
    if useGrp==1 and grpNames != \"\":
        points = ()
        for grp in grpNames.split(\" \"):
            points += geo.globPoints(grp)
        selString = getPointSelection(points)

    if useGrp==2 and grpNames != \"\":
        prims = ()
        for grp in grpNames.split(\" \"):
            prims += geo.globPrims(grp)
        selString = getPrimSelection(prims)
    
    if useGrp==0 and grpNames != \"\":
    
        edges = ()
        for grp in grpNames.split(\" \"):
            edges += geo.globEdges(grp)
        selString = getEdgeSelection(edges)       
    
    script = script.replace(\"selectionString\", selString)

    loadString = \"\"
    if autoLoad:
        if clearUVs:
            loadString = 'ZomLoad({File={Path=\"odfilepath\", ImportGroups=true, XYZ=true}, NormalizeUVW=true})'
        else:
            loadString = 'ZomLoad({File={Path=\"odfilepath\", ImportGroups=true, XYZUVW=true, UVWProps=true}, NormalizeUVW=true})'
    script = script.replace(\"loadString\", loadString.replace(\"odfilepath\", exportFile.replace(\"\\\\\\\", \"/\")))

    saveString = \"\"
    if autoSave:
        saveString = 'ZomSave({File={Path=\"odfilepath\", UVWProps=true}, __UpdateUIObjFileName=true})'
    script = script.replace(\"saveString\", saveString.replace(\"odfilepath\", exportFile.replace(\"\\\\\\\", \"/\")))

    quitString = \"\"
    if autoQuit:
        quitString = 'ZomQuit()'
    script = script.replace(\"quitString\", quitString)    
 
    lua = \"\"
        
    if enableLua:
        lua = luaString
    else:
        rizom_attr = \"rizomuv_cmd\"

        if geo.findGlobalAttrib(rizom_attr):
            lua = geo.attribValue(rizom_attr)
    
    script = script.replace(\"luaString\", lua) 
    
    
    if lua != \"\": 
        f = open(tempfile.gettempdir() + os.sep + \"riz.lua\", \"w\")
        f.write(script)
        f.close()    
        cmd = '\"' + rizomPath + '\" -cfi \"' + tempfile.gettempdir() + os.sep + \"riz.lua\" + '\"'
        if platform.system() == \"Windows\":
            subprocess.call(cmd, shell=False)
        else:
            # This might break OSX, but it's not working on Linux at all 
            #os.system('open -W \"' + rizomPath + '\" --args -i -cfi \"'+tempfile.gettempdir() + os.sep + 'riz.lua\"')
            os.system('\"' + rizomPath + '\" -i -cfi \"'+tempfile.gettempdir() + os.sep + 'riz.lua\"')
 
if os.path.exists(exportFile): 
    os.remove(exportFile)

if geo:
    automaticRoundtrip()
    hou.parm(\"../file1/reload\").pressButton()
    #geo.loadFromFile(exportFile)"	)
}

