--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/Do_Splitting.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	remove_me	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"vector edgev(int h){
    vector src = point(0, \"P\", hedge_srcpoint(0, h));
    vector dst = point(0, \"P\", hedge_dstpoint(0, h));
    return dst - src;
}

// inspired by https://iquilezles.org/www/articles/distfunctions/distfunctions.htm
int on_line( vector p; int edge)
{
  vector a = point(0, \"P\",hedge_srcpoint(0, edge));
  vector b = point(0, \"P\",hedge_dstpoint(0, edge)); 
   
  vector pa = p - a, ba = b - a;
  float h = clamp( dot(pa,ba)/dot(ba,ba), 0.0, 1.0 );
  return  length(pa - ba*h) < 1e-9;
}


int h = i@cut_edge;
int temp = h;

vector src = point(0, \"P\", hedge_srcpoint(0, h));
vector dst = point(0, \"P\", hedge_dstpoint(0, h));
vector edge = dst - src;

float randomness = clamp(ch(\"randomness\"), 0, 1);

randomness = fit01(randomness, .5, .01);
vector rand_p = src + edge * fit01(float(rand(@primnum + detail(1, \"iteration\") + ch(\"seed\"))), randomness, 1 - randomness);


int closest_edge;
vector hit_p;
float min_dist = 1e10;
int i = 0;
int index = -1;
int pts[];
int temp_2 = h;
do{
    int next = hedge_next(0, h);
    vector next_dst = point(0, \"P\", hedge_dstpoint(0, next));
    vector next_src = point(0, \"P\", hedge_srcpoint(0, next));
   
    int hit;
    
    //set up clip plane
    vector4 plane =normalize(edge); 
    plane.w = -dot(normalize(edge), rand_p);

    vector temp_p = rand_p;
    vector temp_src = next_src;
    vector temp_dst = next_dst;
    clip(hit, next_dst, next_src, plane);
    float l = distance(temp_src, next_src);
    if(on_line(next_src, next) && l > 1e-9 && next != temp && distance(rand_p, next_src) < min_dist){
            min_dist = distance(rand_p, next_src);
            hit_p = next_src;
            closest_edge = next;
            index = i + 1;      
    }     
    
    append(pts, hedge_dstpoint(0, h));
    h = next;
    dst = next_dst;
    i++;
}while(h != temp);

if(index == -1){
    @group_remove_me = 0;
    return;
}

int start = addpoint(0, rand_p);
int end = addpoint(0, hit_p);

int prim_1[] = array(start);
append(prim_1, pts[0:index]);
append(prim_1, end);


int prim_2[] =  array(end);
append(prim_2, pts[index : len(pts)]);
append(prim_2, start);
addprim(0, \"poly\", prim_1);
addprim(0, \"poly\", prim_2);"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	64	)
folder1	[ 0	locks=0 ]	(	0	0	)
seed	[ 8	locks=0 ]	(	[ seed	0 ] 	)
randomness	[ 8	locks=0 ]	(	[ randomness	0 ] 	)
