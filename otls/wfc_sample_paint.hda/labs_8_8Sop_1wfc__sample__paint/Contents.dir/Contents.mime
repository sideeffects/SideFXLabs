float stroke_float = f@this_float;
int stroke_int = i@this_int;

int type = prim(1, \"stroke_attribtype\", 0);
if (type == 0) {
    vector old_stencil_value = v@stencil_vector;
    float old_stencil_alpha = f@stencil_alpha;
    float overlay_alpha = 1.0 - f@current_negalpha;
    vector overlay_value = stroke_color;
    f@stencil_alpha = overlay_alpha + old_stencil_alpha * (1 - overlay_alpha);
    v@stencil_vector = vector(lerp(old_stencil_value, overlay_value, overlay_alpha / @stencil_alpha));
} 
else if (type == 1)
{
    float old_stencil_value = f@stencil_float;
