snippet	[ 0	locks=0 ]	(	"int visible = prim(1, \"stroke_visibility\", 0);
float bias = ch(\"bias\");
if (@current_negalpha != 1.0)
{
    matrix toscreen = prim(1, 'stroke_toscreenxform', 0);

    vector4 pos = @P;
    pos.w = 1;
    pos *= toscreen ;
    pos /= pos.w;
    pos.z = -1;
    pos *= invert(toscreen);
    pos /= pos.w;

    vector stroke_orig = set(pos.x, pos.y, pos.z);
    vector this_pt = v@P;
    vector p, uvw;
    vector dir = normalize(stroke_orig - this_pt);
    vector ray_orig = this_pt + bias * dir;
    int primnum = intersect(0, ray_orig, stroke_orig - ray_orig, p, uvw);
    if (primnum >= 0) 
        f@current_negalpha = 1.0;
}
"	)
