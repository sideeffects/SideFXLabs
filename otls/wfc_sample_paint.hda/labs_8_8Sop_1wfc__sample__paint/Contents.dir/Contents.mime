{
version 0.8
python	[ 0	locks=0 ]	(	"node = hou.pwd()
geo = node.geometry()


numobjects = hou.parm(\"../mpPaintObjects\").evalAsInt()
multiparmvals = [\"BLANK\"]

for x in range(numobjects):
    val = hou.parm(\"../sObject_%s\" % int(x+1))
    multiparmvals.append(val.evalAsString())
    
for point in geo.points():
    value = point.attribValue(\"nameindex\")
    
    if value <= len(multiparmvals):
        point.setAttribValue(\"name\", multiparmvals[value])"	)
}

