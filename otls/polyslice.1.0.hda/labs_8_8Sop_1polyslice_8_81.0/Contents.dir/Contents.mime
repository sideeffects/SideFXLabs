snippet	[ 0	locks=0 ]	(	"int minL = detail(0, \"_minClip\");
int maxL = detail(0, \"_maxClip\");

float x = vector(v@_primP).x;
if(x>=maxL){
    i@_sliceID = maxL; 
}
else{
    if(x<minL){
        i@_sliceID = minL - 1;
    }
    else{
        if(primintrinsic(0, \"closed\", @primnum) == 1){
            i@_sliceID = floor(x);
        }
        else{
            i@_sliceID = int(rint(x));
        }       
    }
}    
"	)
