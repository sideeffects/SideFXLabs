Content-Disposition: attachment; filename="hdaroot/SLICER_MAIN/POLYLINE/POLY_WHAT_TO_SLICE3.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_borderPrims	)
grouptype	[ 0	locks=0 ]	(	"prims"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int keepStart = chi(\"Keep_Start\");
int keepEnd = chi(\"Keep_End\");
float bb[] = primintrinsic(0, \"bounds\", @primnum);
int minL = detail(0, \"_minClip\");
int maxL = detail(0, \"_maxClip\");
int clip = 0;
if(bb[1] < minL){
    if(keepStart){
       //setprimgroup(0, \"_minClip\", @primnum,1, \"set\");
    }
    else{
        setprimgroup(0, \"_toRemove\", @primnum, 1, \"set\");
    }
    clip = -1;
}

if(bb[0] > maxL){ 
    if(keepEnd){
        //setprimgroup(0,\"_maxClip\",@primnum,1,\"set\");
    }
    else{
        setprimgroup(0, \"_toRemove\", @primnum, 1, \"set\");
    }
    clip = 1;   
}

if(clip == 0){
    if(ceil(bb[1]) - floor(bb[0])>1){
        setprimgroup(0, \"_toSlice\", @primnum, 1, \"set\");
    }
}      
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
