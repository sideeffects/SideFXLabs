snippet	[ 0	locks=0 ]	(	"int keepStart = chi(\"Keep_Start\");
int keepEnd = chi(\"Keep_End\");
float bb[] = primintrinsic(0,\"bounds\",@primnum);
int minL = detail(0,\"_minClip\")+keepStart;
int maxL = detail(0,\"_maxClip\")-keepEnd;
int clip = 0;

if(bb[1] < minL){
    setprimgroup(0,\"_toRemove\",@primnum,1,\"set\");
    clip = -1;
}

if(bb[0] > maxL){ 
    setprimgroup(0,\"_toRemove\",@primnum,1,\"set\");
    clip = 1;  
}

if(clip == 0){
