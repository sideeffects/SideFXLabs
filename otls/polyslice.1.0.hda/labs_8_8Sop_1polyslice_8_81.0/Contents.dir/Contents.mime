snippet	[ 0	locks=0 ]	(	"//////////////////////////
// decalre variables
int sn, slice, curHE;
float sStepSpace, eStepSpace, dotV;
vector sPos, ePos, dir;
int setSlice[];
vector axis = {1,0,0};
int sPt, ePt, sVx, eVx, sL;
int i;

//////////////////////////
// get half edges
int HEs[], pts[], slices[], sliceVals[];

//get valid prims
int pt;
int startHE = primhedge(0, @primnum);
curHE = startHE;
do{       
    curHE = hedge_next(0, curHE);
    if(curHE == -1){
    
        // a polyline creates a bad last HE. This removes it
        pop(HEs);
        insert(HEs, 0, startHE);
        break;
    }
    append(HEs,curHE); 
}while(curHE != startHE);

//////////////////////
// order half edges
// find min x pos of all source ptd of half edges
float minX;
float curX;
int sliceI = 0;
sPt = hedge_srcpoint(0, HEs[0]);
minX = vector(point(0, \"P\", sPt)).x;
i = 1;

for(i = 1; i<len(HEs); i++){
    sPt = hedge_srcpoint(0, HEs[i]);
    curX = vector(point(0, \"P\", sPt)).x;
    if(curX < minX){
        sliceI = i;
        minX = curX;
    }
}

// reorder array so min x is first.
if(sliceI!=0){
    int sHEs[] = slice(HEs, sliceI, len(HEs));
    int eHEs[] = slice(HEs, 0, sliceI);
    append(sHEs, eHEs);
    HEs = sHEs;
}

i[]@__HEs = HEs;"	)
