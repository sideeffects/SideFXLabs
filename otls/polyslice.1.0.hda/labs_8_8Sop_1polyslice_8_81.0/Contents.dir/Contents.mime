snippet	[ 0	locks=0 ]	(	"// get all attrs
string attrs[] = detailintrinsic(1,\"vertexattributes\");
string attr;

foreach(attr;attrs){
    // check for internal atts - attrs with leading underscore
    if(match(\"_*\",attr)){
    continue;
    }
    
    int type = attribtype(1,\"vertex\",attr);
    int size = attribsize(1,\"vertex\",attr);
    // if int
    
    if(type==0){
        int val = vertex(1,attr,i@_sVx);
        setvertexattrib(0,attr,-1,@vtxnum,val);  
        continue;
    }
    
    // vector/float
    if(type==1){
        if(size==1){
            float val = lerp(vertex(1,attr,i@_sVx),vertex(1,attr,i@_eVx),f@_w);
            setvertexattrib(0,attr,-1,@vtxnum,val);
            continue;
        }
        if(size==2){
            vector2 val = lerp(vertex(1,attr,i@_sVx),vertex(1,attr,i@_eVx),f@_w);
            setvertexattrib(0,attr,-1,@vtxnum,val);
            continue;
        }            
        if(size==3){
            vector val = lerp(vertex(1,attr,i@_sVx),vertex(1,attr,i@_eVx),f@_w);
            setvertexattrib(0,attr,-1,@vtxnum,val);
            continue;
        }            
        if(size==4){
            vector4 val = lerp(vertex(1,attr,i@_sVx),vertex(1,attr,i@_eVx),f@_w);
            setvertexattrib(0,attr,-1,@vtxnum,val);
            continue;
        }
    }    
    //if string
    if(type==2){
        string val = vertex(1,attr,i@_sVx);
        setvertexattrib(0,attr,-1,@vtxnum,val);
        continue;
    }
}"	)
