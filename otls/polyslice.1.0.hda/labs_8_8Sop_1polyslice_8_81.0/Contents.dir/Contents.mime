Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_toSlice	)
grouptype	[ 0	locks=0 ]	(	"prims"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// functions
// create sliced poly wind
void windPoly(int pts[], slices[], sliceVals[], primnum; string polyType){    
    int numPts = len(pts) - 1;
    int b = numPts + 1;
    int i = 0;
    int wind[];
    int primID;
    
    // main wind loop
    // i is the forward index could and b is the backwards index count
    while(i < b){
        wind = array();  
        int prevI = i;
        int prevB = b;
        int toWind = 1;
        i ++;
        b --;        
        int noWind = 0;       
        while((sliceVals[i] == sliceVals[prevI] && slices[prevI] == 1)|| slices[i] != 1){
            if(i >= b){
                toWind = 0;
                break;
            }
            i ++;
        }
        while(slices[b] != 1 || sliceVals[i] != sliceVals[b]){
            if(i >= b){
                toWind = 0; 
                break;
            }
            b --;
        }
        
        // form wind
        if(polyType == \"polyline\"){
            if(toWind && i != b){
                wind = array(pts[i], pts[b]);
            }
            else{
                continue;
            }
        }               
        else
            if(toWind){
                wind = pts[prevI:i+1];
                int temp[] = pts[b:prevB+1];
                append(wind, temp); 
            }
            else{
                wind = pts[prevI:prevB+1];
            }      
        primID = addprim(0, polyType, wind);
        setprimattrib(0,\"_primId\",primID,primnum);
    }
}


//////////////////////////////////////////////////////////////////
//user input
float thresh = ch(\"threshold\");
int sliceNeigh= chi(\"slice_neighbouring_edge\");

//////////////////////////////////////////////////////////////////
//declare variables
int sn, slice, curHE;
float sStepSpace, eStepSpace, dotV;
vector sPos, ePos, dir;
int sPt, ePt, sVx, eVx, sL;
int i;
int polylineOut = 0;
string polyType = \"poly\";

//////////////////////////////////////////////////////////////////
// get half edges
int HEs[] = i[]@__HEs;
int pts[], slices[], sliceVals[];


//////////////////////////////////////////////////////////////////
// step 
int sliceCount = 0;
int totalPts = 0;
int isPartial = 0;
int minClip = detail(0, \"_minClip\") + (polylineOut * chi(\"Keep_Min\"));
int maxClip = detail(0, \"_maxClip\") - (polylineOut * chi(\"Keep_Max\"));
foreach(i, curHE; HEs){
    // get geo info
    sPt = hedge_srcpoint(0, curHE);     
    ePt = hedge_dstpoint(0, curHE);
    sVx = hedge_srcvertex(0, curHE);
    eVx = hedge_dstvertex(0, curHE);
    sPos = point(0, \"P\", sPt); 
    ePos = point(0, \"P\", ePt);    

    dir = ePos - sPos;
    sn = int(sign(dir.x));
    if(sn == 0){
        sn = 1;
        }    
        
    sStepSpace = sPos.x * sn;
    eStepSpace = ePos.x * sn;
    float sliceDist = sPos.x - rint(sPos.x);
    int isSlice = abs(sliceDist) < thresh && (sPos.x >= minClip && sPos.x <= maxClip) ;
    float skip = sliceDist * sn < 0;
    slice = int(ceil(sStepSpace)) + isSlice;  
    int newPt; 
    newPt = addpoint(0, sPos);
    append(pts, newPt);
    append(slices, isSlice);
    append(sliceVals, int(rint(sPos.x)));  
    
    if(isSlice){
    sliceCount ++;
    }
    
    // create attr for attr copy
    setpointattrib(0, \"_sPt\", newPt, sPt);
    setpointattrib(0, \"_ePt\", newPt, ePt);   
    setpointattrib(0, \"_sVx\", newPt, sVx);
    setpointattrib(0, \"_eVx\", newPt, eVx);
    setpointattrib(0, \"_w\", newPt, 0.0);
            
    if(sliceNeigh == 1){
        if(inprimgroup(0, \"_borderPrims\",@primnum)){ 
            isPartial = 1;
            continue;
            }
        }
       
    // reusable dot value use for projectio
    dotV = 1 / dot(dir, {1, 0 , 0});

    slice = int(ceil(sStepSpace)) + (sliceDist * sn < 0) * isSlice;
    while(slice < eStepSpace - thresh){

        // projection ratio
        float ratio = (slice - (sStepSpace)) * dotV * sn;
        vector stepPos = (dir * ratio) + sPos;          
        if(stepPos.x >= minClip - 0.5 && stepPos.x <= maxClip + 0.5){        
            // create point
            newPt = addpoint(0, stepPos);
        
            // add attributes to copy attributes later     
            setpointattrib(0, \"_sPt\", newPt, sPt);
            setpointattrib(0, \"_ePt\", newPt, ePt);
            setpointattrib(0, \"_sVx\", newPt, sVx);
            setpointattrib(0, \"_eVx\", newPt, eVx);
            setpointattrib(0, \"_w\", newPt, ratio);
                
            int put = slice * sn;
            append(sliceVals, int(rint(stepPos.x)));
            append(slices, 1);
            append(pts, newPt);
            totalPts ++;
            sliceCount ++;
        }
        slice ++;
    }
}

int pt;

//////////////////////////////////////////////////////////////////
// wind polygon
// init vars
int numPts = len(pts) - 1;
int sliceVal;
float sSl = sliceVals[0];
sPt = pts[0];
int makePrim = 0;

if(sliceCount > 1){
    makePrim = 1;
}


// call maind winding method
if(makePrim && !isPartial){
    windPoly(pts,slices,sliceVals,@primnum,polyType);
    removeprim(0,@primnum,1);
}
if(isPartial){
    int primId = addprim(0,\"poly\",pts);
    setprimattrib(0,\"_primId\",primId,@primnum); 
    setprimgroup(0,\"_borderPrims\",primId,1,\"set\");
    removeprim(0,@primnum,1);
}"	)
