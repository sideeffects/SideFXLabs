{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"matrix make_xform(const vector pos; const vector nml; const vector refpos)
{
    vector ref = refpos - pos;
    vector axisx = normalize(nml);
    vector axisy = normalize(ref - dot(ref, axisx) * axisx);
    vector axisz = cross(axisx, axisy);
    matrix xform = set(axisx, axisy, axisz);
    translate(xform, pos);
    return xform;
}

int next_pt = neighbour(0, @ptnum, 0);
4@diff = make_xform(point(1, \"P\", @ptnum), point(1, chs(\"./n_attr\"), @ptnum), point(1, \"P\", next_pt));
4@diff *= invert(make_xform(v@P, v@N, point(0, \"P\", next_pt)));
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
