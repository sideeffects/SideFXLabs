vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"matrix make_xform(const vector pos; const vector nml; const vector refpos)
{
    vector ref = refpos - pos;
    vector axisx = normalize(nml);
    vector axisy = normalize(ref - dot(ref, axisx) * axisx);
    vector axisz = cross(axisx, axisy);
    matrix xform = set(axisx, axisy, axisz);
    translate(xform, pos);
    return xform;
}

int next_pt = neighbour(0, @ptnum, 0);
matrix diff = point(1, \"diff\", @ptnum);
vector norm = point(0,chs(\"n_attr\"),i@ptnum);
matrix m = make_xform(v@P, norm, point(0, \"P\", next_pt));
matrix xform = invert(m) * diff * m;

//4@xform = xform;
addattrib(0,\"point\",chs(\"xform_attr\"),xform);
setpointattrib(0,chs(\"xform_attr\"),i@ptnum,xform);


//v@P *= xform;
//v@N *= matrix3(xform);"	)
