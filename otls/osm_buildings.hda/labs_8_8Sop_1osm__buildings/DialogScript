    parm {
        name    "level_height"
        label   "Standard Level Height"
        type    float
        default { "4" }
        range   { 0 10 }
        parmtag { "script_callback_language" "python" }
    }
    parm {
        name    "shrink_factor"
        label   "Shrink Footprints"
        type    float
        default { "0" }
        range   { 0 10 }
    }
    parm {
        name    "boolean_buildings"
        label   "Boolean Buildings"
        type    toggle
        default { "0" }
    }
    parm {
        name    "visualize_buildings"
        label   "Visualize Buildings"
        type    toggle
        default { "0" }
    }
    parm {
        name    "gen_nodata"
        label   "Generate Buildings on Missing Data"
        type    toggle
        default { "0" }
    }
    parm {
        name    "seed"
        label   "Random Seed"
        type    float
        default { "0" }
        disablewhen "{ gen_nodata == 0 }"
        range   { 0 10 }
    }
    parm {
        name    "height_range"
        label   "Height Range"
        type    float_minmax
        size    2
        default { "10" "30" }
        disablewhen "{ gen_nodata == 0 }"
        range   { 0 10 }
    }
    parm {
        name    "up_axis"
        label   "Up Axis"
        type    vector
        size    3
        default { "0" "1" "0" }
        range   { -1 1 }
        parmtag { "script_callback_language" "python" }
    }
}
