snippet	[ 0	locks=0 ]	(	"int hedge = vertexhedge(0, @vtxnum);
int dst;
int cur_hedge = hedge;
int sib = -1;
int found;
do{  
    dst = hedge_srcvertex(0, cur_hedge);
    if(!invertexgroup(0, \"_pwu_ambiguous\", dst)){
        sib = vertex(0, \"_pwu_lookup\", dst);
        found = 1;
        break;
    }
    cur_hedge = hedge_next(0, cur_hedge); 
}while(hedge != cur_hedge);
int seg_prim = vertexprim(1, sib);
int seg_match;
int resolved = -1;
int sib_dist = primvertexcount(1, seg_prim);
float cur_dist = -1;
int vtxs[] = i[]@_pwu_vtxs;
int vtx_lookup;
int vtx;
int resolves[];
foreach(vtx; vtxs){
    seg_match = vertexprim(1, vtx);
    if(seg_match == seg_prim){
        append(resolves, vtx);
    }   
}

if(len(resolves)==1){
    resolved = resolves[0];
}
int sib_lookup;
if(len(resolves)>1){
    sib_lookup = vertex(1, \"_pwu_vtxI\", sib);
    foreach(int r; resolves){
        vtx_lookup = vertex(1, \"_pwu_vtxI\", r);
        if(abs(vtx_lookup - sib_lookup)==1){
            resolved = r;
        }
    }
}


i@_pwu_lookup = resolved; 

"	)
