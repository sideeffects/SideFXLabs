Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_pwu_cap	)
grouptype	[ 0	locks=0 ]	(	"prims"	)
class	[ 0	locks=0 ]	(	"vertex"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"float perim, scale;


if(ch(\"mode\") == 2){
    perim = $PI * 2 * point(0, \"_pwu_width\", @ptnum) * ch(\"texelScale\");
    scale = 0.5 / $PI;
}

else{
    perim = 1;
    scale = 0.5 / $PI;
}

float u = (@uv.x - 0.5) / perim;
float drive_u = (u - 0.5) * $PI * 2;
float xCirc = sin(drive_u) *- scale;
float yCirc = cos(drive_u) * scale;
int dir = (@uv.y == 0) * 2 - 1;
float new_x = ( xCirc * perim)+ 0.5;
float new_y = @uv.y + ((yCirc * perim) + (scale * perim)) * -dir; 
vector new_uv = set(new_x, new_y, @uv.z);

float weld = ch(\"weld\") / i@_pwu_borders;
if(distance(new_uv, @uv) >= weld * perim){
@uv = new_uv;
}"	)
