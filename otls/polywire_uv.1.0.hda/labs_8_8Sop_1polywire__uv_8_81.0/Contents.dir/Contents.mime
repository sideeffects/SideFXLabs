{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"float u = vertex(0, \"_pwu_curve_u\", @vtxnum);
int seg_id = prim(0, \"_pwu_id_seg\", @primnum);
int for_pt, back_pt , idx;
float for_weight, back_weight;

int n_vtx = primvertexcount(1, seg_id) - 1;
float f_w = ch(\"bias\");
float u_w = frac(u * float(n_vtx));
float weight = f_w / ( ((1/u_w) - 2) * (1 - f_w) + 1);
float test_w = u * float(n_vtx) %1;
int ns[];
float ws[];

if(test_w == 0.0){
    idx = int(rint(u * n_vtx));
    for_pt = vertexpoint(1, vertexindex(1, seg_id, idx));
    for_weight = 1-weight;  
    ns = array(for_pt);
    ws = array(1);
}
else{
    idx = int(ceil( u * n_vtx));
    for_pt = vertexpoint(1, vertexindex(1, seg_id, idx));
    for_weight = weight;
    idx = int(floor( u * n_vtx));
    back_pt = vertexpoint(1, vertexindex(1, seg_id, idx));
    back_weight = 1-weight;
    ns = array(back_pt, for_pt);
    ws = array(back_weight, for_weight);    
    
}

i[]@boneCapture_index = ns;
f[]@boneCapture_data = ws;

"	)
