vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int vtxs[] = pointvertices(0, @ptnum);
float tot_u = 0;
int i = 0;
float cur_u;
foreach(int vtx; vtxs){
    cur_u = vertex(0, \"_pwu_u\", vtx);
    if(cur_u != 0.0 && cur_u != 1.0){
        tot_u += vertex(0, \"_pwu_u\", vtx);
        i ++;
    }
}


float pot_u = tot_u / i;
float max_dist = ch(\"max_dist\" )/ i@_pwu_borders;
if(abs(pot_u - f@_pwu_u) <= max_dist){
    f@_pwu_u = pot_u;
    if(i@_pwu_moved == 0 ){
        setdetailattrib(0, \"_pwu_stop_snap\", 0);
        i@_pwu_moved = 1;
    }
}


"	)
