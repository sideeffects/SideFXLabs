snippet	[ 0	locks=0 ]	(	"int cur_pt = @ptnum;
int ns[];
int potential = -1;
float cur_v, v;
float step_v;
float score = -2;
float cur_score;
do{ 
    setpointgroup(0, \"_pwu_spine\", cur_pt,1);
    v = point(0, \"_pwu_pt_v\", cur_pt);
    score = -2;
    cur_score = point(0, \"_pwu_score\", cur_pt);
    ns = neighbours(0, cur_pt);
    potential = -1;
    step_v = 1;
    foreach(int n; ns){
        cur_score = point(0, \"_pwu_score\", n);
        cur_v = point(0, \"_pwu_pt_v\", n);       
        if(cur_v > v && cur_score >= score){
            potential = n;
            score = cur_score;
        }
    }
    cur_pt = potential;
}while(potential != -1);

cur_pt = @ptnum; 
potential = -1;
score = -2;
int i = 0;
do{ 
    setpointgroup(0, \"_pwu_spine\", cur_pt, 1);
    v = point(0,\"_pwu_pt_v\", cur_pt);
    score = -2;
    ns = neighbours(0, cur_pt);
    potential = -1;
    step_v = 1;
    foreach(int n; ns){
        cur_score = point(0, \"_pwu_score\", n);
        cur_v = point(0,\"_pwu_pt_v\", n);       
        if(cur_v < v && cur_score >= score){
            potential = n;
            score = cur_score;
        }
    i += 1;
    }
    cur_pt = potential;
}while(potential != -1);
"	)
