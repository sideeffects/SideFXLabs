--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/make_u/walk_u/walk_u1.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	_pwu_spine	)
grouptype	[ 0	locks=0 ]	(	"points"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int maxIter = chi(\"maxIter\");
int init_pt = @ptnum;
int cur_pt = init_pt;
int next_pt;
float cur_v, offset_u;
vector cur_dir, n_offset, prev_pos; 

float u = 0;
int pts[] = array(); 
int last_pt = -1;
int i = 0;
float score = -2;
float cur_score;

vector up = vertex(0, \"_pwu_vtxUp\",  @vtxnum);
vector tan = vertex(0, \"_pwu_vtxTan\",  @vtxnum);
cur_dir = cross(tan, v@_pwu_surfN);


prev_pos = point(0, \"P\", cur_pt);

do{
    append(pts, cur_pt);        
    int ns[] = neighbours(0, cur_pt);
    cur_v = point(0, \"_pwu_pt_v\", cur_pt);
    score = -2;
    foreach(int n; ns){
    
        // skip if not in row
        if(point(0, \"_pwu_pt_v\", n) != cur_v){
            continue;
        }
        if(last_pt == n){
            continue;
        }
        if(i == 0){        
            //find correct direction for point
            n_offset = normalize(point(0, \"P\", n) - prev_pos);
            cur_score = dot(cur_dir , n_offset);
            if(cur_score >= score){
                score = cur_score;
                next_pt = n;
                continue;
            }
        }
        else{
        next_pt = n;
        }
          
    }
    
    // break if no neighbours found
    if(next_pt==-1){
        break;
    }
    // set vals for next iter
    prev_pos = point(0, \"P\", cur_pt);
    last_pt = cur_pt;
    cur_pt = next_pt;  
    cur_dir = n_offset;
    i += 1;
}while(cur_pt != init_pt && i < maxIter);

// check if set by parent, if not set.
int num_pts = len(pts);
int pt;
foreach(i, pt; pts){
    u = float(i)/float(num_pts);
    setpointattrib(0, \"_pwu_u\", pt, u);
    setpointattrib(0, \"_pwu_borders\", pt, num_pts);
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
maxIter	[ 8	locks=0 ]	(	[ maxIter	0 ] 	)
}

