float cur_u = f@_pwu_u;
float cur_v = f@_pwu_v;
float test_u, test_v;
int vtxs[];
int test_seg;
int cur_seg = prim(0,\"_pwu_id_seg\",@primnum);

// get all fused vtxs for current vtx.
int vtxs_con[] = pointvertices(0, @ptnum);
int vtxs_fuse[];
foreach(int vtx; vtxs_con){
    test_v = vertex(0, \"_pwu_v\", vtx);
    test_seg = prim(0, \"_pwu_id_seg\", vertexprim(0, vtx));
    if(test_v == cur_v && cur_seg == test_seg){
        append(vtxs_fuse, vtx);
    }

}
int hedge = 0;
int v_nxt;
int siblings[] = array(@vtxnum);
int found = 0;
foreach(int vtx; vtxs_fuse){
    hedge = vertexhedge(0, vtx);
    v_nxt = hedge_dstvertex(0, hedge);
    test_v = vertex(0, \"_pwu_v\", v_nxt);
    if(test_v == cur_v){
        append(siblings, v_nxt);
        continue;
    }
    hedge = hedge_prev(0, hedge);
    v_nxt = hedge_srcvertex(0, hedge);    
    test_v = vertex(0, \"_pwu_v\", v_nxt);
    if(test_v == cur_v){
        append(siblings, v_nxt);
    }    
}

i[]@_pwu_siblings = siblings;

