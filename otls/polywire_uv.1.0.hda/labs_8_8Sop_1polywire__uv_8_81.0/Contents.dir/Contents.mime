--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/curve_frame/roll_relax/laplacian_approx.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	!`chs(\"../../../constraingroup\")`	)
grouptype	[ 0	locks=0 ]	(	"points"	)
class	[ 0	locks=0 ]	(	"vertex"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int iter = detail(1, \"iteration\");
float collapse = ch(\"collapse_thresh\");

//get average up for all eq verts
int eq_vtxs[] = i[]@_pwu_vtx_eq;
vector cur_up = set(0,0,0);

// get averag
foreach(int vtx; eq_vtxs){
    cur_up += vertex(0, \"_pwu_vtxUp\", vtx);
}
cur_up /= len(eq_vtxs);
// rendom on collapsed average
if(length(cur_up) < collapse){
    cur_up = normalize(rand(@vtxnum + 345 + iter));
}
cur_up = normalize(cur_up);


int n_vtxs[] = i[]@_pwu_vtx_ns;
int l = len(n_vtxs);
vector totUp = set(0,0,0);

foreach(int vtx; n_vtxs){
    totUp += vertex(0, \"_pwu_vtxUp\", vtx);     
}

vector ave = totUp/l;
if(length(ave) < collapse ){   
    ave = normalize(rand(@vtxnum + 40 + iter));
}

vector new_up = (ave + v@_pwu_vtxUp) * 0.5;

new_up = normalize(new_up);

