bindprim3	[ 0	locks=0 ]	(	0	)
bindname3	[ 0	locks=0 ]	(	Ce.z	)
bindprimname4	[ 0	locks=0 ]	(	[ bindprimname4	"" ] 	)
bindprim4	[ 0	locks=0 ]	(	0	)
bindname4	[ 0	locks=0 ]	(	Ce	)
}

