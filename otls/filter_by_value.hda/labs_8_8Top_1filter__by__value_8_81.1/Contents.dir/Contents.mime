    parm {
        name    "invert"
        label   "Invert"
        type    toggle
        default { "0" }
        parmtag { "script_callback_language" "python" }
    }
    parm {
        name    "sAttrName"
        label   "Atribute Name"
        type    string
        default { "" }
        parmtag { "script_callback_language" "python" }
    }
    parm {
        name    "iCombineMode"
        label   "Combine Mode"
        type    ordinal
        default { "0" }
        menu {
            "0" "Union"
            "1" "Intersection"
        }
        parmtag { "script_callback_language" "python" }
    }
    multiparm {
        name    "iValueMultiParm"
        label    "Values"

        parm {
            name    "iValueEnabled_#"
            label   "Enabled"
            type    toggle
            nolabel
            joinnext
            default { "1" }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "iOperation_#"
            label   "Operation"
            type    ordinal
            joinnext
            default { "0" }
            menu {
                "0" "Equal To"
                "1" "Not Equal To"
                "2" "Greater Than"
                "3" "Less Than"
                "4" "Starts With"
                "5" "Ends With"
                "6" "Contains"
            }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "sValue_#"
