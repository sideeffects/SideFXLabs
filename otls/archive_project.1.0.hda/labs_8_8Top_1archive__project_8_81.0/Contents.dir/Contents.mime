import os

#empty dictionary
assets = {}

#empty value holders
hip_list, abc_list, usd_list, geo_list, hda_list, img_list, misc_list = ([] for i in range(7))

#key list
ident_list = [\"hip\", \"abc\", \"usd\", \"geo\", \"hda\", \"img\", \"misc\"]

#iterate and compile list of assets and append them to the right value holder lists
for i, upstream_item in enumerate(upstream_items):
    item_list = {}
    
    file_type = upstream_item.attribValue(\"file_type\")
    file_name = upstream_item.attribValue(\"file_name\")
    dest_file_path = os.path.normpath(os.path.join(upstream_item.attribValue(\"pack_destpath\"), file_name))
    source_path = os.path.normpath(upstream_item.attribValue(\"source_path\"))
    dest_path = upstream_item.attribValue(\"dest_path\")
    parm_name = upstream_item.attribValue(\"parm_name\")
    parm_path = upstream_item.attribValue(\"parm_path\")
    is_seq = bool(upstream_item.attribValue(\"seq_split\"))
    

    if file_type == \"hip\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path}
        hip_list.append(item_list)
    elif file_type == \"hda\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path}
        hda_list.append(item_list)
    elif file_type == \"abc\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path, 'parm_name': parm_name, 'parm_path': parm_path}
        abc_list.append(item_list)
    elif file_type == \"usd\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path, 'parm_name': parm_name, 'parm_path': parm_path}
        usd_list.append(item_list)
    elif file_type == \"Geometry\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path, 'parm_name': parm_name, 'parm_path': parm_path, 'is_sequence' : is_seq}
        geo_list.append(item_list)
    elif file_type == \"Image\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path, 'parm_name': parm_name, 'parm_path': parm_path}
        img_list.append(item_list)
    elif file_type == \"Any\":
        item_list = {'file_name': file_name, 'source_path': source_path, 'dest_path': dest_file_path, 'parm_name': parm_name, 'parm_path': parm_path}
        misc_list.append(item_list)       

#assign list based on ident
value_list = [hip_list, abc_list, usd_list, geo_list, hda_list, img_list, misc_list]

#build asset dict if valid
for i, value in enumerate(value_list):
    if len(value)<1:
        pass
    else:
        assets[ident_list[i]] = value

