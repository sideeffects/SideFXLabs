script	[ 0	locks=0 ]	(	"import os
import hou

hip_value = hou.text.expandString(\"\\$HIP\")

hou.hipFile.load(work_item.stringAttribValue(\"hip_file\"), ignore_load_warnings=True)

type_list = work_item.stringAttribArray(\"file_type\")
parms = work_item.stringAttribArray(\"parm_path\")
value = work_item.stringAttribArray(\"parm_value\")
hipdir = work_item.stringAttribValue(\"hip_dir\")


count = work_item.intAttribValue(\"asset_count\")

has_hda = 0

for i in range(count):
    if type_list[i] != \"hda\":
        parm = hou.parm(parms[i])
        parm_value = value[i].replace(hip_value, \"\\$HIP\")
        try:
            parm.set(parm_value)
        except:
            print(\"Set Failed on %s:\", parm)
        
    else:
        has_hda+=1
        hda_path = parms[i]
        hda_path = os.path.split(hda_path)        
        hda_path = os.path.join(hipdir, hda_path[1])
        

if has_hda>0:
        
    if os.path.isdir(hda_path):        
        hda_files = os.listdir(hda_path)
    
        for files in hda_files:
            f_path = os.path.join(hda_path, files)
        try:    
            hou.hda.installFile(f_path)
        except:
            print(\"HDA install failed\")
        
hou.hipFile.save()

"	)
