for upstream_item in upstream_items:
    dest_path = upstream_item.attribValue(\"pack_destpath\")

#gather parms and paths
def gatherFileDependencies():
    hipdir = os.environ[\"HIP\"]
    parms = []
    paths = []
    extn = []
    for parm, path in hou.fileReferences():
        parms.append(parm)
        paths.append(path)
    
    return parms, paths

#build extensions and make corrections
def buildExtension(paths):
    extension = []
    for i, path in enumerate(paths):
        ext_string = os.path.splitext(str(paths[i]))[1]
        #check for bgeo .sc/.gz extensions - but this is a temp fix
        if(ext_string==\".sc\" or ext_string==\".gz\"):
            ext_string=\".bgeo\"+ext_string
        
        extension.append(ext_string)
    
        
    return extension

#get filetype information    
def getFileType(parms):
    ftype = []
    for i, parm in enumerate(parms):
        if parm:
            list_type = parm.parmTemplate().fileType()
            ftype.append(str(list_type).rsplit(\".\")[1])
        else:
            ftype.append(\"hda\")
        
    return ftype
    
#get parent node
def getParentNode(parms):
    node = []
    for i, parm in enumerate(parms):
        if parm:
            parent = parm.node()
            node.append(str(parent))
        else:
            node.append(\"None\")
            
    return node
    
#get parameter data    
def getParmInfo(parms):
    parm_path = []
    parm_name = []
    for i, parm in enumerate(parms):
        if parm:
            path = parm.path()
            name = parm.name()
            parm_path.append(str(path))
            parm_name.append(str(name))
        else:
            parm_path.append(\"None\")
            parm_name.append(\"None\")
    return parm_path, parm_name
            
parms, paths = gatherFileDependencies()
extn = buildExtension(paths)
file_type = getFileType(parms)
nodes = getParentNode(parms)
parm_path, parm_name = getParmInfo(parms)


#create workitems        
for i, extn in enumerate(extn):
    if extn:
        new_work_item = item_holder.addWorkItem(index=i)
        new_work_item.setStringAttrib(\"parm_path\", parm_path[i])
        new_work_item.setStringAttrib(\"parm_name\", parm_name[i])
        new_work_item.setStringAttrib(\"source_path\", paths[i])
        new_work_item.setStringAttrib(\"file_ext\", extn)
        new_work_item.setStringAttrib(\"file_type\", file_type[i])
        new_work_item.setStringAttrib(\"nodename_dir\", nodes[i])
        new_work_item.setStringAttrib(\"copy_dest_path\", dest_path)
